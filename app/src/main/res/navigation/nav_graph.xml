            android:id="@+id/action_fragment_organizaciones_to_frag_info_orgs"
            app:destination="@id/frag_info_orgs" />
    </fragment>
    <fragment
        android:id="@+id/fragment_curiosidades"
        android:name="com.example.animalcare.fragments.Fragment_Curiosidades"
        android:label="fragment_curiosidades"
        tools:layout="@layout/fragment_curiosidades" />
    <fragment
        android:id="@+id/fragment_enfermedades"
        android:name="com.example.animalcare.fragments.Fragment_Enfermedades"
        android:label="fragment_enfermedades"
        tools:layout="@layout/fragment_enfermedades">
        <action
            android:id="@+id/action_fragment_enfermedades_to_frag_info_enf"
            app:destination="@id/frag_info_enf" />
    </fragment>
    <fragment
        android:id="@+id/fragment_veterinarias"
        android:name="com.example.animalcare.fragments.Fragment_Veterinarias"
        android:label="fragment_veterinarias"
        tools:layout="@layout/fragment_veterinarias">
        <action
            android:id="@+id/action_fragment_veterinarias_to_frag_info_vet"
            app:destination="@id/frag_info_vet" />
    </fragment>
    <fragment
        android:id="@+id/fragment_Animal"
        android:name="com.example.animalcare.fragments.Fragment_Animal"
        android:label="fragment_animal"
        tools:layout="@layout/fragment_animal">
        <action
            android:id="@+id/action_fragment_Animal_to_frag_info_perro"
            app:destination="@id/frag_info_perro" />
    </fragment>
    <fragment
        android:id="@+id/fragment_Gato"
        android:name="com.example.animalcare.fragments.Fragment_Gato"
        android:label="fragment_gato"
        tools:layout="@layout/fragment_gato">
        <action
            android:id="@+id/action_fragment_Gato_to_frag_info_gato"
            app:destination="@id/frag_info_gato" />
    </fragment>
    <fragment
        android:id="@+id/fragment_especies"
        android:name="com.example.animalcare.fragments.Fragment_Especies"
        android:label="fragment_especies"
        tools:layout="@layout/fragment_especies">
        <action
            android:id="@+id/action_fragment_Especies_to_fragment_Gato"
            app:destination="@id/fragment_Gato" />
        <action
            android:id="@+id/action_fragment_Especies_to_fragment_Animal"
            app:destination="@id/fragment_Animal" />
    </fragment>
    <fragment
        android:id="@+id/frag_info_perro"
        android:name="com.example.animalcare.fragments_info.frag_info_perro"
        android:label="fragment_frag_info_perro"
        tools:layout="@layout/fragment_frag_info_perro" />
    <fragment
        android:id="@+id/frag_info_gato"
        android:name="com.example.animalcare.fragments_info.frag_info_gato"
        android:label="fragment_frag_info_gato"
        tools:layout="@layout/fragment_frag_info_gato" />
    <fragment
        android:id="@+id/frag_info_vet"
        android:name="com.example.animalcare.fragments_info.frag_info_vet"
        android:label="fragment_frag_info_vet"
        tools:layout="@layout/fragment_frag_info_vet" />
    <fragment
        android:id="@+id/frag_info_enf"
        android:name="com.example.animalcare.fragments_info.frag_info_enf"
