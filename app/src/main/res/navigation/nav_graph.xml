        tools:layout="@layout/fragment_frag_info_enf" />
    <fragment
        android:id="@+id/frag_info_orgs"
        android:name="com.example.animalcare.fragments_info.frag_info_orgs"
        android:label="fragment_frag_info_orgs"
        tools:layout="@layout/fragment_frag_info_orgs" />
