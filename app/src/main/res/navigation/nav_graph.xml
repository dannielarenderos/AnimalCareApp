<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragment_Main">
    <fragment
        android:id="@+id/fragment_Main"
        android:name="com.example.animalcare.fragments.Fragment_Main"
        android:label="fragment_fragment__main"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_fragment_Main_to_fragment_leyes"
            app:destination="@id/fragment_leyes" />

        <action
            android:id="@+id/action_fragment_Main_to_fragment_organizaciones"
            app:destination="@id/fragment_organizaciones" />
        <action
            android:id="@+id/action_fragment_Main_to_fragment_curiosidades"
            app:destination="@id/fragment_curiosidades" />
        <action
            android:id="@+id/action_fragment_Main_to_fragment_enfermedades"
            app:destination="@id/fragment_enfermedades" />
        <action
            android:id="@+id/action_fragment_Main_to_fragment_veterinarias"
            app:destination="@id/fragment_veterinarias" />
        <action
            android:id="@+id/action_fragment_Main_to_fragment_especies"
            app:destination="@id/fragment_especies" />
    </fragment>
    <fragment
        android:id="@+id/fragment_leyes"
        android:name="com.example.animalcare.fragments.Fragment_Leyes"
        android:label="fragment_leyes"
        tools:layout="@layout/fragment_leyes" />
