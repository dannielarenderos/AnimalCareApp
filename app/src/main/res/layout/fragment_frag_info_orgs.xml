<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:background="#FFF"
             tools:context=".fragments_info.frag_info_orgs">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <ImageView
                android:src="@drawable/icon_org"
                android:layout_width="164dp"
                android:layout_height="136dp" android:id="@+id/img_organizacion" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintVertical_bias="0.117"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/tv_nombreOrga" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/img_organizacion"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:textSize="27sp"
                android:gravity="center" app:layout_constraintVertical_bias="0.069"/>
        <ImageView
                android:src="@drawable/icon_tel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/imageView4" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_nombreOrga"
                app:layout_constraintEnd_toStartOf="@+id/tv_telefonoOrga" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.537" app:layout_constraintVertical_bias="0.196"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/tv_telefonoOrga"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:textSize="20sp"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tv_nombreOrga"
                app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.204"/>
        <ImageView
                android:src="@drawable/icon_dire"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/imageView7"
                app:layout_constraintEnd_toStartOf="@+id/tv_direccionOrga" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView4" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.58"
                app:layout_constraintVertical_bias="0.112"/>
        <TextView
                android:text="TextView"
                android:layout_width="230dp"
                android:layout_height="wrap_content" android:id="@+id/tv_direccionOrga"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tv_telefonoOrga"
                app:layout_constraintHorizontal_bias="0.885" app:layout_constraintVertical_bias="0.192"
                android:textSize="20sp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>