<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".fragments.frag_info_vet"
             android:background="@color/colorPrimaryDark"
             >

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_margin="10dp"
            android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="349dp"
                android:layout_height="551dp"
                android:layout_margin="10dp"
                android:background="#fff"
                android:layout_marginBottom="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.738"
                android:id="@+id/constraintLayout" app:layout_constraintHorizontal_bias="0.511">

        </androidx.constraintlayout.widget.ConstraintLayout>
        <ImageView
                android:background="#FFF"
                android:src="@drawable/icon_vet"
                android:layout_width="164dp"
                android:layout_height="140dp" android:id="@+id/img_vet"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintVertical_bias="0.079" app:layout_constraintHorizontal_bias="0.49"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/tv_nomVet" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
                app:layout_constraintHorizontal_bias="0.587" android:textSize="30sp" android:gravity="center"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/img_vet"
                app:layout_constraintVertical_bias="0.044"/>
        <TextView
                android:text="TextView"
                android:layout_width="249dp"
                android:layout_height="wrap_content"
                android:id="@+id/tv_telVet"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="20sp" android:gravity="center" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_nomVet" app:layout_constraintVertical_bias="0.179"
                app:layout_constraintStart_toEndOf="@+id/imageView5" android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.0"/>
        <TextView
                android:text="TextView"
                android:layout_width="249dp"
                android:layout_height="wrap_content" android:id="@+id/tv_direVet"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:textSize="20sp"
                android:gravity="center_horizontal" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_telVet" android:layout_marginEnd="16dp"
                android:layout_marginRight="16dp" app:layout_constraintVertical_bias="0.247"
                app:layout_constraintStart_toEndOf="@+id/imageView6" android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.0"/>
        <ImageView
                android:src="@drawable/icon_tel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/imageView5"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tv_nomVet"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="52dp"
                android:layout_marginStart="52dp"
                app:layout_constraintVertical_bias="0.157"/>
        <ImageView
                android:src="@drawable/icon_dire"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/imageView6" android:layout_marginBottom="128dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="52dp"
                android:layout_marginStart="52dp" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_telVet" app:layout_constraintVertical_bias="0.398"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>